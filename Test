---

frida-ps -U | grep cfl

--

cat > hafas.js <<'JS'
Java.perform(function () {
  var Request = Java.use("okhttp3.Request");
  var Buffer = Java.use("okio.Buffer");

  Request.newBuilder.implementation = function () {
    var req = this.build();
    try {
      console.log("=== HTTP REQUEST ===");
      console.log("URL:", req.url().toString());
      console.log("METHOD:", req.method());
      console.log("HEADERS:", req.headers().toString());

      var body = req.body();
      if (body) {
        var buffer = Buffer.$new();
        body.writeTo(buffer);
        console.log("BODY:", buffer.readUtf8());
      }
    } catch (e) {
      console.log("Error:", e);
    }
    return this;
  };
});
JS

---

frida -U -f com.cfl.mobile.pub -l hafas.js --no-pause

