TS=$(date +%F_%H-%M-%S)
LOG="/sdcard/cfl_watch/logs/map_${TS}.log"
mkdir -p /sdcard/cfl_watch/logs

bash /sdcard/cfl_watch/map_run.sh --depth 1 --max-screens 5 --max-actions 3 2>&1 | tee "$LOG"
echo "LOG=$LOG"

----

ls -1dt /sdcard/cfl_watch/map/* 2>/dev/null | head -n 5 || echo "=> aucun run dans /sdcard/cfl_watch/map/"

----

sed -n '1,40p' /sdcard/cfl_watch/map.sh

-----

tail -n 80 "$LOG"
