pkg install -y android-tools
adb version

---

su -c 'setprop service.adb.tcp.port 5555; setprop ctl.restart adbd'

---

su -c 'getprop service.adb.tcp.port'

--

adb start-server
adb connect 127.0.0.1:5555
adb devices

---
adb shell id -Z
adb shell input tap 100 100
echo "rc=$?"

---

su -c 'setprop service.adb.tcp.port -1; setprop ctl.restart adbd'
